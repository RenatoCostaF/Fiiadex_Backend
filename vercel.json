{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        },
        {
            "src": "prisma/schema.prisma",
            "use": "npx prisma generate"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/routes.ts"
        }
    ]
}